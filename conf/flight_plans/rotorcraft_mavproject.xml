<!DOCTYPE flight_plan SYSTEM "flight_plan.dtd">

<flight_plan alt="1.5" ground_alt="0" lat0="51.990634" lon0="4.376789" max_dist_from_home="15" name="team3" security_height="1">
  <header>
#include "autopilot.h"
#include "navigation.h"
#include "guidance/guidance_h.h"
</header>
  <waypoints>
    <waypoint name="HOME" x="0.0" y="0.0"/>
    <waypoint name="CLIMB" x="0.0" y="5.0"/>
    <waypoint name="STDBY" x="0.7" y="-0.7"/>
    <waypoint name="TD" x="-0.6" y="-1.2"/>
    <waypoint name="FROM" x="0.3" y="-2.3"/>
    <waypoint name="TO" x="1.6" y="-1.4"/>
    <waypoint lat="51.990628" lon="4.376856" name="FA1"/>
    <waypoint lat="51.990601" lon="4.376779" name="FA2"/>
    <waypoint lat="51.990644" lon="4.376736" name="FA3"/>
    <waypoint lat="51.990672" lon="4.376812" name="FA4"/>
    <waypoint lat="51.990624" lon="4.376867" name="C1"/>
    <waypoint lat="51.990593" lon="4.376779" name="C2"/>
    <waypoint lat="51.990646" lon="4.376724" name="C3"/>
    <waypoint lat="51.990677" lon="4.376812" name="C4"/>
  </waypoints>
  <sectors>
    <sector color="red" name="Flight_Area">
      <corner name="FA1"/>
      <corner name="FA2"/>
      <corner name="FA3"/>
      <corner name="FA4"/>
    </sector>
  </sectors>
  <exceptions>
    <exception cond="autopilot_flight_time == 595" deroute="land here"/>
  </exceptions>
  <blocks>
    <block name="Wait GPS">
      <call fun="NavKillThrottle()"/>
      <while cond="!GpsFixValid()"/>
    </block>
    <block name="Geo init">
      <while cond="LessThan(NavBlockTime(), 5)"/>
      <call fun="NavSetWaypointHere(WP_HOME)"/>
      <call fun="NavSetWaypointHere(WP_CLIMB)"/>
      <call fun="NavSetWaypointHere(WP_STDBY)"/>
      <call fun="NavSetWaypointHere(WP_TD)"/>
      <call fun="NavSetWaypointHere(WP_FROM)"/>
      <call fun="NavSetWaypointHere(WP_TO)"/>
      <call fun="NavSetAltitudeReferenceHere()"/>
    </block>
    <block name="Holding point">
      <call fun="NavKillThrottle()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block name="Start Engine">
      <call fun="NavResurrect()"/>
      <attitude pitch="0" roll="0" throttle="0" until="stage_time>2" vmode="throttle"/>
    </block>
    <block name="Takeoff" strip_button="Takeoff" strip_icon="takeoff.png">
      <exception cond="stateGetPositionEnu_f()->z > 1.3" deroute="Standby"/>
      <call fun="NavSetWaypointHere(WP_CLIMB)"/>
      <stay climb="0.9" vmode="climb" wp="CLIMB"/>
    </block>
    <block name="Standby" strip_button="Standby" strip_icon="home.png">
      <call fun="NavSetWaypointHere(WP_STDBY)"/>
      <stay wp="STDBY"/>
    </block>
    <block name="Obstacle Avoidance">
      <exception cond="!InsideFlight_Area(GetPosX(), GetPosY())" deroute="OA Turn Around"/>
      <while cond="TRUE">
        <go from="FROM" hmode="route" wp="TO"/>
      </while>
    </block>
    <block name="OA Turn Around">
      <call fun="NavSetWaypointHere(WP_FROM)"/>
      <call fun="nav_set_heading_rad(ANGLE_FLOAT_OF_BFP(nav_heading)+2.6)"/>
      <stay until="stage_time>1" wp="FROM"/>
      <go from="FROM" hmode="route" until="stage_time>1" wp="TO"/>
      <deroute block="Obstacle Avoidance"/>
    </block>
    <block name="Obstacle stop turn left">
      <call fun="nav_set_heading_rad(ANGLE_FLOAT_OF_BFP(nav_heading)-1.4)"/>
      <stay until="stage_time>1" wp="FROM"/>
      <deroute block="Obstacle Avoidance"/>
    </block>
    <block name="Obstacle stop turn right">
      <call fun="nav_set_heading_rad(ANGLE_FLOAT_OF_BFP(nav_heading)+1.4)"/>
      <stay until="stage_time>1" wp="FROM"/>
      <deroute block="Obstacle Avoidance"/>
    </block>
    <block name="land here" strip_button="Land Here" strip_icon="land-right.png">
      <call fun="NavSetWaypointHere(WP_TD)"/>
      <stay until="stage_time>2" wp="TD"/>
    </block>
    <block name="flare">
      <exception cond="NavDetectGround()" deroute="Holding point"/>
      <exception cond="!nav_is_in_flight()" deroute="landed"/>
      <call fun="NavStartDetectGround()"/>
      <stay climb="-0.8" until="stage_time > 4" vmode="climb" wp="TD"/>
    </block>
    <block name="landed">
      <attitude pitch="0" roll="0" throttle="0" until="stage_time>1" vmode="throttle"/>
      <call fun="NavKillThrottle()"/>
    </block>
  </blocks>
</flight_plan>
